generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Kurse {
  id           Int    @id @default(autoincrement())
  name         String
  beschreibung String
  startDatum   String
  endDatum     String
  preis        Float
  dauer        String
  bild         String
}
